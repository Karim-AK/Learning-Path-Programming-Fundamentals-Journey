

#include <iostream>
#include <string>
using namespace std;

void SwapNumbers(int &A, int &B) // Procedure
{
	int Temp;

	Temp = A;
	A = B;
	B = Temp;

	cout << "After swap inside the fanction A= " << A << " B = " << B << endl;
	
}



int main()
{
	;int A, B;

	cout << "Please enter A?\n";
	cin >> A;


	cout << "Please enter B?\n";
	cin >> B;

	cout << "Before swap A= " << A << " B= " << B << endl;

	SwapNumbers(A, B);

	cout << "After swap inside Main function A= " << A << " B= " << B << endl;


	return 0;
}
